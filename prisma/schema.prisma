model Activity {
  id              String   @id @default(cuid())
  googlePlaceId   String   @unique
  name            String
  description     String
  category        String
  address         String
  latitude        Float
  longitude       Float
  rating          Float
  priceLevel      Int
  photoReferences String[]
  openingHours    String[]
  website         String?
  phoneNumber     String?
  reviews         Json[]
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
} 